<div class="Content">
    <section class="personalSection">
        <header class="routineHeader" id="routineHeader">
            <h1 class="username-exercise">{{this.name}}</h1>
        </header>
        <div class="allExerciseCards">
        {{#each exercises }}
        <div class="exercise-card">
            <div class="exercise-card-content">
                <div class="buttons-exer">
                    <button class="ex-move-btn">Move to</button>
                    <button class="ex-delete-btn">X</button>
                </div>
                <div class="ex-card-main">
                    <h2>{{this.name}}</h2>
                    <h5>{{this.description}}</h5>
                    <h5>{{this.intensity}}</h5>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    </section>
</div>